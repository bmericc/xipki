<?xml version="1.0" encoding="UTF-8"?>
<PKCS11Conf xmlns="http://xipki.org/commons/security/pkcs11/conf/v2">
  <modules>
    <!-- exactly one module must be named 'default' -->
    <module name="default">
      <nativeLibraries>
        <nativeLibrary>
          <path>url=https://localhost:9443/p11proxy/default</path>
        </nativeLibrary>
      </nativeLibraries>
      <includeSlots>
        <slot>
          <index>0</index>
        </slot>
      </includeSlots>
      <readonly>false</readonly>
    </module>
  </modules>
</PKCS11Conf>
