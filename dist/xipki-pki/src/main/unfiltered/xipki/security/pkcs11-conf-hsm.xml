<?xml version="1.0" encoding="UTF-8"?>
<PKCS11Conf xmlns="http://xipki.org/commons/security/pkcs11/conf/v2">
  <modules defaultModule="softhsm">
    <module name="softhsm">
      <nativeLibraries>
        <nativeLibrary>
          <path>/usr/local/lib/softhsm/libsofthsm2.so</path>
        </nativeLibrary>
      </nativeLibraries>
      <includeSlots>
        <slot>
          <index>0</index>
        </slot>
      </includeSlots>
      <passwords>
        <password>
          <singlePassword>123456</singlePassword>
        </password>
      </passwords>
    </module>
